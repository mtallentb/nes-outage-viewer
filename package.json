{
  "name": "nes-tracker",
  "version": "1.0.0",
  "description": "CLI tool to track NES power outage status near your home",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc && cp -r src/public dist/",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "dev:cli": "ts-node src/index.ts",
    "dev:watch": "ts-node src/index.ts --watch"
  },
  "keywords": ["nes", "outage", "power", "tracker"],
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  }
}
